{
  "name": "release_gha",
  "version": "4.8.0",
  "description": "",
  "main": "index.js",
  "config": {
    "aws": {
      "region": "us-east-1",
      "s3": {
        "bucket": "membership-preferences-ui-deploy"
      }
    },
    "output": "dist"
  },
  "scripts": {
    "build": "mkdir --parent ${npm_package_config_output} && cp *.js ${npm_package_config_output}",
    "s3-deploy": "s3-deploy '${npm_package_config_output}/**' --cwd '${npm_package_config_output}' --filePrefix ${npm_package_name}-${npm_package_version} --region ${npm_package_config_aws_region} --bucket ${npm_package_config_aws_s3_bucket}",
    "echo": "echo ${npm_package_name}-${npm_package_version} ${npm_package_config_aws_region} ${npm_package_config_aws_s3_bucket}"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jest": "^28.1.1"
  },
  "devDependencies": {
    "s3-deploy": "^1.4.0"
  }
}
